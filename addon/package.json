{
  "name": "@ember/test-waiters",
  "version": "3.0.2",
  "description": "Allow tests to be aware of addon / application asynchrony.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/emberjs/ember-test-waiters",
  "license": "MIT",
  "contributors": [
    "Steve Calvert <steve.calvert@gmail.com>",
    "Robert Jackson <me@rwjblue.com>"
  ],
  "scripts": {
    "build": "rollup --config ./rollup.config.mjs",
    "start": "rollup --config --watch",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:prettier": "prettier --check .",
    "lint:js": "eslint --ext ts,js .",
    "lint:js:fix": "eslint --ext ts,js . --fix",
    "lint:ts": "tsc --noEmit --emitDeclarationOnly false",
    "prepare": "pnpm build",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.7",
    "@glimmer/env": "^0.1.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.24.5",
    "@embroider/addon-dev": "^4.3.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@tsconfig/ember": "^3.0.6",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "decorator-transforms": "^2.0.0",
    "ember-source": "~5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.0.2",
    "eslint-plugin-n": "^17.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "execa": "^8.0.1",
    "fix-bad-declaration-output": "^1.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "qunit": "^2.20.1",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "~5.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./__private__/types": {
      "types": "./declarations/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./__private__/token": {
      "types": "./declarations/token.d.ts",
      "default": "./dist/token.js"
    }
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
